{
  "manifest_version": 2,
  "name": "My Shortcuts",
  "description": "This Extension shows the most used shortcuts",
  "version": "0.1",

  "browser_action": {
     "default_icon": "logo.png",
     "default_popup": "shortcuts.html"
  },
  "permissions": [
          "storage"
        ],
  "content_scripts": [{
      "matches": ["http://*/*", "https://*/*"],
      "js": ["js/jquery-1.12.3.min.js"],
      "css":["css/main.css"]
   }]
}
